#!/bin/sh

# Show help info when user type below commands.
# $ ioszip
# $ ioszip -h
# $ ioszip -help
if [ $# -eq 0 ] || [ $1 = '-h' ] || [ $1 = '--help' ]; then
    cat help.txt
    exit 1 #Failure!
fi

if [ $# -eq 1 ] && [ $1 = 'open' ]; then
    open 'http://ioszip.mashroom.in/'
    exit 0 #Success!
fi
